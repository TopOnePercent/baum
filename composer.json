{
    "name": "toponepercent/baum",
    "type": "library",
    "description": "Baum is an implementation of the Nested Set pattern for Eloquent models.",
    "keywords": ["nested set", "mptt", "laravel", "eloquent", "database"],
    "license": "MIT",
    "authors": [
        {
            "name": "Estanislau Trepat",
            "email": "estanis@etrepat.com",
            "homepage": "http://etrepat.com"
        },
        {
            "name": "Gary Pearman",
            "email": "gaz@red-robot.co.uk",
            "homepage": "http://red-robot.co.uk"
        },
        {
            "name": "TOP ONE PERCENT LTD",
            "email": "developers@theforesight.co.uk",
            "homepage": "http://theforesight.co.uk"
        }
    ],
    "prefer-stable": true,
    "require": {
        "php": "^7.2|^8.0",
        "illuminate/console": "~5.8.0|^6.0|^7.0|^8.0|^9.0",
        "illuminate/database": "~5.8.0|^6.0|^7.0|^8.0|^9.0",
        "illuminate/events": "~5.8.0|^6.0|^7.0|^8.0|^9.0",
        "illuminate/filesystem": "~5.8.0|^6.0|^7.0|^8.0|^9.0",
        "illuminate/support": "~5.8.0|^6.0|^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5",
        "php-coveralls/php-coveralls": "^2.2"
    },
    "autoload": {
        "psr-0": {
            "Baum": "src/"
        },
        "files": [
            "src/helpers/tree_helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Baum\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Baum\\Providers\\BaumServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": [
            "@php ./vendor/bin/phpunit"
        ]
    }
}
